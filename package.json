{
  "name": "clipanion-generator-command",
  "version": "1.0.1",
  "description": "TypeScript library template",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./FileExistsError": "./dist/FileExistsError.js",
    "./Generator": "./dist/Generator.js",
    "./GeneratorCommand": "./dist/GeneratorCommand.js"
  },
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rimraf dist",
    "prepare": "husky",
    "start": "concurrently --names es 'yarn tsc --watch --preserveWatchOutput'",
    "test": "vitest test --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johngeorgewright/clipanion-generator-command.git"
  },
  "keywords": [],
  "author": "John Wright <johngeorge.wright@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johngeorgewright/clipanion-generator-command/issues"
  },
  "homepage": "https://github.com/johngeorgewright/clipanion-generator-command#readme",
  "devDependencies": {
    "@commitlint/cli": "19.6.0",
    "@commitlint/config-conventional": "19.6.0",
    "@commitlint/types": "19.5.0",
    "@semantic-release/commit-analyzer": "13.0.0",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.1",
    "@semantic-release/release-notes-generator": "14.0.1",
    "@types/node": "22.10.1",
    "@types/semantic-release": "20.0.6",
    "@types/source-map-support": "0.5.10",
    "@vitest/coverage-v8": "2.1.8",
    "clipanion": "^4.0.0-rc.4",
    "commitizen": "4.3.1",
    "concurrently": "9.1.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "9.1.7",
    "lint-staged": "15.2.10",
    "nodemon": "3.1.7",
    "prettier": "3.4.1",
    "renamer": "5.0.2",
    "rimraf": "6.0.1",
    "semantic-release": "24.2.0",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.2",
    "typescript": "5.7.2",
    "vitest": "2.1.8"
  },
  "lint-staged": {
    "*.{md,json,js,jsx,ts,tsx,yml,yaml}": [
      "yarn prettier --ignore-path .gitignore --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "enquirer": "^2.4.1",
    "tslib": "^2.8.1"
  },
  "packageManager": "yarn@4.5.3",
  "peerDependencies": {
    "clipanion": "^4.0.0-rc.4"
  }
}